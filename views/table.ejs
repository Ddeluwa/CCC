<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>수목센서 관리 사이트</title>

    <!-- Custom fonts for this template-->
    <link href="/stylesheets/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link
        href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
        rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="/stylesheets/style.css" rel="stylesheet">
</head>

<body>
<% if(DataType === "4D") {%>
    <table border="1" align="center" width="100%" id = "4D">
        <tr align="center">
            <th>Serial No.</th>
            <th>Record No.</th>
            <th>Device Type</th>
            <th>Timestamp</th>
            <th colspan="2">Before Heating Temp.</th>
            <th colspan="2">After Heating Temp.</th>    
            <th>Growth Rate</th>
            <th>Battery Voltage</th>
            <th>Air Humidity</th>
            <th>Air Temperature</th>
            <th>Tree Stability</th>
            <th>Stem Water Content</th>
        </tr>
        <tr align="center"> 
            <td id="data-SerialNumber"><%= SerialNumber %></td>
            <td id="data-RecordNumber"><%= RecordNumber %></td>
            <td id="data-DeviceType"><%= DataType %></td>
            <td id="data-Timestamp"><%= Timestamp %></td>
            <td id="data-BrefTemperature" ><%= (Before_Tref).toFixed(2) %> &#8451; </td>
            <td id="data-BHTemperature" ><%= (Before_Theat).toFixed(2) %> &#8451; </td>
            <td id="data-AHTemperature"><%= (After_Tref).toFixed(2) %> &#8451; </td>
            <td id="data-AHTemperature"><%= (After_Theat).toFixed(2) %> &#8451; </td>
            <td id="data-GrowthRate"><%= (GrowthRate).toFixed(2) %> mm</td>
            <td id="data-BattVolt"><%= (BattVoltage).toFixed(2) %> mV</td>
            <td id="data-AirHumidity"><%= AirHumid %> %</td>
            <td id="data-AirTemperature"><%= AirTemp %> &#8451;</td>
            <td id="data-Stability"><%= (Stability).toFixed(2) %> &#186;</td>
            <td id="data-Stem"><%=(Stem).toFixed(2)%> %</td>
        </tr>
    </table>
<%} else if(DataType === "49"){%>
    <table border="1" align="center" width = "100%" id = "49">
        <tr align="center">
            <tr align="center">
                <td colspan = "4">Common</td>
                <td colspan = "6">Near Infrared</td>
                <td colspan = "6">Visible Light Spectrum</td>
            </tr>
            <tr align="center">
                <th>Serial No.</th>
                <th>Record No.</th>
                <th>Device Type</th>
                <th>Timestamp</th>
                <th>650nm</th>
                <th>680nm</th>    
                <th>730nm</th>
                <th>760nm</th>
                <th>810nm</th>
                <th>860nm</th>
                <th>450nm</th>
                <th>500nm</th>
                <th>550nm</th>
                <th>570nm</th>
                <th>600nm</th>
                <th>650nm</th>
            </tr>
            <tr align="center">
                <td id="data-SerialNumber"><%= SerialNumber %></td>
                <td id="data-RecordNumber"><%= RecordNumber %></td>
                <td id="data-DeviceType"><%= DataType %></td>
                <td id="data-Timestamp"><%= Timestamp %></td>
                <td id="data-NI650"><%= (NI610).toFixed(4) %></td>
                <td id="data-NI680"><%= (NI680).toFixed(4) %></td>
                <td id="data-NI730"><%= (NI730).toFixed(4) %></td>
                <td id="data-NI760"><%= (NI760).toFixed(4) %></td>
                <td id="data-NI810"><%= (NI810).toFixed(4) %></td>
                <td id="data-NI860"><%= (NI860).toFixed(4) %></td>
                <td id="data-VLS450"><%= (VLS450).toFixed(4) %></td>
                <td id="data-VLS500"><%= (VLS500).toFixed(4) %></td>
                <td id="data-VLS550"><%= (VLS550).toFixed(4) %></td>
                <td id="data-VLS570"><%= (VLS570).toFixed(4) %></td>
                <td id="data-VLS600"><%= (VLS600).toFixed(4) %></td>
                <td id="data-VLS650"><%= (VLS650).toFixed(4) %></td>
            </tr>
        </tr>
    </table>
<%} else if(DataType === "4B"){%>
    <table border="1" align="center" width = "100%" id = "4B">
        <tr align="center">
            <tr align="center">
                <th>Serial No.</th>
                <th>Record No.</th>
                <th>Device Type</th>
                <th>Timestamp</th>
                <th>Battery Level</th>
                <th>Firmware Version</th>    
            </tr>
            <tr align="center">
                <td id="data-SerialNumber"><%= SerialNumber %></td>
                <td id="data-RecordNumber"><%= RecordNumber %></td>
                <td id="data-DeviceType"><%= DataType %></td>
                <td id="data-Timestamp"><%= Timestamp %></td>
                <td id="data-BattLevel"><%= BatteryLevel %> mV</td>
                <td id="data-FirmLevel"><%= FirmwareVersion %></td>
            </tr>
        </tr>
    </table>
<%} else if(DataType === "4C"){%>
    <table border="1" align="center" width = "100%" id = "4C">
        <tr align="center">
            <tr align="center">
                <th>Serial No.</th>
                <th>Record No.</th>
                <th>Device Type</th>
                <th>Timestamp</th>
                <th>RSSI First</th>
                <th>RSSI Second</th>    
            </tr>
            <tr align="center">
                <td id="data-SerialNumber"><%= SerialNumber %></td>
                <td id="data-RecordNumber"><%= RecordNumber %></td>
                <td id="data-DeviceType"><%= DataType %></td>
                <td id="data-Timestamp"><%= Timestamp %></td>
                <td id="data-RSSIFirst"><%= RSSI_first %></td>
                <td id="data-RSSISecond"><%= RSSI_second %></td>
            </tr>
            
        </tr>
        <script>
            
        </script>
    </table>
<% } %>
</body>